<div class="container">
    <div class="page-header">
        <h1>List of adoptable pets at {{shelter}}</h1>
    </div>
    {{#each pets as |pet|}}
       <li class="list-group-item">
           <h4>{{pet.name.$t}}</h4><br>
            <p>{{pet.age.$t}} | {{pet.sex.$t}} | {{pet.size.$t}}</p>
       </li>
    {{/each}}
</div>